<!DOCTYPE html>
<html>
    <head>
      <%= javascript_include_tag :defaults %>
      <title>St Judes Management Application</title>
      <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
      <%= javascript_include_tag 'applications', 'data-turbolinks-track' => true %>
      <%= csrf_meta_tags %>
    </head>

    <body>
        <%= render "layouts/header" %>

        <div class="container">

          <section class="round">
            <%= yield %>
          </section>

        </div>
        <%= render "layouts/footer" %>
    </body>
</html>